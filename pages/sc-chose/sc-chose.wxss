page{
  background: #fff;
}
.shangchuan{
  width: 100%;
}
.shangchuanCon{
  padding: 19rpx 26rpx;
  display: flex;
  justify-content: space-between;
}
.shangchuanLeft{
  width: 570rpx;
}
.choseLable{
  font-size: 32rpx;
  color: #424242;
  font-weight: bold;
}
.lables{
  width: 100%;
  display: flex;
  justify-content:space-between;
  flex-wrap: wrap;
  margin-bottom: 29rpx;
}
.lableli{
width:21%;
text-align: center;
}
.lableList{
height:auto;
padding: 15rpx 0;
background:#CFCFCF;
border-radius:10rpx;
font-size: 28rpx;
color: #FFFFFF;
margin-top: 30rpx;
}
.question-title{
  padding: 0 20rpx;
height:76rpx;
background:#F1F1F1;
border-radius:4rpx;
margin-bottom: 10rpx;
}
.weui-cell__bd{
  height:auto;
  padding: 27rpx 19rpx;
  background: #F1F1F1;
  margin-bottom: 29rpx;
  position: relative;
}
.weui-textarea-counter{
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.weui-textarea{
  width: 472rpx;
}
.addImg{
  font-size: 30rpx;
  color: #424242;
}
.question-images{
  display: flex;
  justify-content:space-between;
  width: 100%;
  height: 100rpx;
}
.q-image{
  width: 100rpx;
  height: 100rpx;
}
.shangchuanRight{
  text-align: center;
height:60rpx;
line-height: 60rpx;
background:#489BFE;
border-radius:4rpx;
font-size: 28rpx;
color: #FFFFFF;
font-weight: bold;
margin: 0;
}
.active-tag .lableList {
  background: #489BFE;
  color: #fff;
}
.uploads{
  display: flex;
  justify-content: space-between;
}
.upload{
  width: auto;
  padding: 15rpx 25rpx;
  color: #fff;
  background: #3393FF;
  border-radius: 10rpx;
  font-size: 24rpx;
}